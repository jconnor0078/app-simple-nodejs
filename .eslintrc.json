{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "linebreak-style":"off",
        "eol-last":"off",
        "comma-dangle": "off",
        "object-curly-newline":"off",
        "no-underscore-dangle": "off",
        "no-throw-literal":"off"
    }
}
